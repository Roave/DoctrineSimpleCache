{
    "name": "roave/doctrine-simplecache",
    "description": "Doctrine Cache adapter for PSR-16 Simple Cache",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/cache": "^1.7",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "cache/integration-tests": "dev-master",
        "cache/tag-interop": "dev-master",
        "infection/infection": "^0.20.0",
        "roave/infection-static-analysis-plugin": "^1.2",
        "phpunit/phpunit": "^8.4",
        "symfony/console": "^4.3",
        "symfony/phpunit-bridge": "^4.4@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Roave\\DoctrineSimpleCache\\": "src/"
        },
        "files": [
            "namespace-bc-aliases.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RoaveTestAsset\\DoctrineSimpleCache\\": "test/asset/",
            "RoaveTest\\DoctrineSimpleCache\\": "test/unit/"
        }
    },
    "provide": {
        "psr/simple-cache-implementation": "1.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
